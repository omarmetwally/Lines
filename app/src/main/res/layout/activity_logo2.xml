<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/loogo2"
    tools:context="com.example.omarm.lines.logo2">

    <EditText
        android:id="@+id/textView30"
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="152dp"
        android:hint="MLETTERLOGO"
        android:gravity="center"
        android:textColor="#095344"
        android:textColorHint="#095344"
        android:inputType="textPersonName"
        android:textSize="30sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView30"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="-152dp"
        android:gravity="center"
        android:hint="SLOGAN GOES HERE"
        android:textColor="#00A69C"
        android:textColorHint="#00A69C"
        android:inputType="textPersonName"
        android:textSize="20sp" />

    <Button
        android:id="@+id/save"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="27dp"
        android:layout_marginTop="15dp"
        android:text="save" />
</RelativeLayout>
